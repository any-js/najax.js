{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "includeDate": false,
    "sort": false,
    "systemName": "najax.js",
    "footer" : "&nbsp;",
    "navType": "vertical",
    "theme": "yeti",
    "linenums" : true,
    "collapseSymbols": false,
    "inverseNav": true,
    "highlightTutorialCode": true,
    "protocol": "html://"
  },
  "markdown"  : {
    "parser"   : "gfm",
    "hardwrap": true
  },
  "opts": {
    "template": "/usr/lib/node_modules/ink-docstrap/template",
    "encoding": "utf8"
  }
}
